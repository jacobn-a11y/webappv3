{
  "name": "storyengine",
  "version": "0.1.0",
  "description": "B2B SaaS - Consolidate call recordings into Account Journey views with AI-powered story extraction",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:test:push": "DATABASE_URL=$TEST_DATABASE_URL prisma db push --skip-generate"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^3.0.0",
    "@prisma/client": "^5.20.0",
    "@workos-inc/node": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.20.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "fuse.js": "^7.0.0",
    "helmet": "^8.0.0",
    "markdown-it": "^14.0.0",
    "openai": "^4.70.0",
    "psl": "^1.9.0",
    "stripe": "^17.0.0",
    "winston": "^3.14.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/markdown-it": "^14.0.0",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.0.0",
    "prisma": "^5.20.0",
    "supertest": "^7.2.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.18"
  }
}
